{
	"name": "cloudoki/oauth2-stack",
	"description": "A complete Oauth2 Server and Views stack",
	"homepage": "http://github.com/Cloudoki/oauth2-stack",
	"keywords": ["oauth2","laravel"],
    "license":"MIT",
	"authors": [
		{
			"name": "Koen Betsens",
			"email": "koen@cloudoki.com"
		},
		{
			"name": "Rui Molefas",
			"email": "rui@cloudoki.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "5.1.*",
		"bshaffer/oauth2-server-php": "v1.6",
		"koenbetsens/jobserver": "dev-lvel5"
	},
	"autoload": {
		"classmap": [
			"src/controllers",
			"src/lang",
			"src/migrations",
			"src/models",
			"src/seeds",
			"src/views"
		],
		"psr-0": {
			"Cloudoki\\OaStack\\": "src/"
		}
	},
	"minimum-stability": "stable"
}